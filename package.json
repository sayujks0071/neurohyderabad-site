    "seo:audit": "node scripts/seo/audit.mjs",
    "seo:report": "node scripts/seo/report.mjs",
    "seo:all": "npm run seo:audit && npm run seo:report",
    "verify:deployment": "node scripts/verify-deployment.js",
    "check:sitemap": "node scripts/check-sitemap.js",
