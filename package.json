{
  "name": "neurosurgery-nextjs-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "next-sitemap && node scripts/generate-image-sitemap.js && node scripts/generate-video-sitemap.js",
    "start": "next start",
    "promote:draft": "ts-node scripts/promote-draft.ts",
    "check:schemas": "ts-node scripts/check-schemas.ts",
    "check:lighthouse": "ts-node scripts/check-lighthouse.ts",
    "lhci": "lhci autorun",
