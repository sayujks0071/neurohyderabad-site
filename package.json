    "flags": "^4.0.2",
    "googleapis": "^170.0.0",
    "hypertune": "^2.10.1",
    "ics": "^3.8.1",
    "inngest": "^3.49.3",
    "jsdom": "^27.4.0",
    "lucide-react": "^0.544.0",
    "next": "16.2.0-canary.6",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.52.2",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "remotion": "^4.0.286",
    "resend": "^6.2.2",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4",
    "vitest": "^4.0.16",
    "web-vitals": "^5.1.0",
    "workflow": "4.0.1-beta.50",
    "yup": "^1.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
