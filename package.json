    "@vercel/og": "^0.8.5",
    "ai": "^5.0.106",
    "critters": "^0.0.23",
    "flags": "^4.0.2",
    "hypertune": "^2.10.1",
    "ics": "^3.8.1",
    "inngest": "^3.44.2",
    "lucide-react": "^0.544.0",
    "next": "15.5.9",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.52.2",
    "react-markdown": "^10.1.0",
    "resend": "^6.2.2",
    "server-only": "^0.0.1",
    "web-vitals": "^5.1.0",
    "workflow": "^4.0.1-beta.32",
    "yup": "^1.4.0"
