  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.17",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "gray-matter": "^4.0.3",
    "lighthouse": "^13.0.1",
    "linkinator": "^6.1.4",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.2.3",
    "opentype.js": "^1.3.4",
    "png-to-ico": "^3.0.1",
