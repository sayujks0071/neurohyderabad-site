{
  "name": "neurosurgery-nextjs-site",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build --webpack",
    "postbuild": "node scripts/generate-image-sitemap.js && node scripts/generate-video-sitemap.js",
    "lint": "next lint",
    "test": "vitest run",
    "start": "next start",
    "promote:draft": "ts-node scripts/promote-draft.ts",
    "check:schemas": "node scripts/check-schemas.ts",
    "check:lighthouse": "node scripts/check-lighthouse.ts",
    "lhci": "lhci autorun",
    "seo:lh": "lhci autorun --collect.url=https://www.drsayuj.com --upload.target=temporary-public-storage",
    "seo:audit": "node scripts/comprehensive-seo-audit.js",
    "health:check": "node scripts/health-seo-check.js",
    "health:full": "node scripts/health-seo-check.js --full",
    "verify:deployment": "node scripts/verify-deployment.js",
    "check:sitemap": "node scripts/check-sitemap.js",
    "verify:opengraph": "npx tsx scripts/verify-opengraph.ts",
    "sandbox:test": "node scripts/sandbox-test.js",
    "gsc:verify": "npx tsx scripts/verify-gsc-setup.ts",
    "gsc:list": "npx tsx scripts/google-seo-automation.ts --list",
    "gsc:status": "npx tsx scripts/google-seo-automation.ts --status",
    "gsc:inspect": "npx tsx scripts/google-seo-automation.ts --inspect",
    "gsc:submit": "npx tsx scripts/google-seo-automation.ts --sitemap",
    "gsc:submit-all": "npx tsx scripts/submit-all-sitemaps.ts",
    "gsc:index": "npx tsx scripts/google-seo-automation.ts --index",
    "gsc:index-priority": "npx tsx scripts/index-priority-urls.ts",
    "gsc:index-all": "npx tsx scripts/submit-all-urls.ts",
    "sitemap:submit": "npx tsx scripts/submit-sitemap-and-index.ts --sitemap-only",
    "sitemap:index": "npx tsx scripts/submit-sitemap-and-index.ts --index-only",
    "sitemap:update": "npx tsx scripts/submit-sitemap-and-index.ts",
    "middleware:setup": "tsx scripts/setup-middleware-monitoring.ts",
    "middleware:alerts": "tsx scripts/setup-middleware-alerts.ts",
    "jules:weekly-seo": "npx tsx scripts/jules-weekly-seo-automation.ts",
    "jules:test": "npx tsx scripts/test-jules-api.ts",
    "analytics:bigquery-insights": "npx tsx scripts/ga4-bigquery-insights.ts",
    "reindex:gemini": "ts-node scripts/reindex-gemini-rag.ts",
    "new:blog": "tsx scripts/new-blog.ts",
    "normalize:blog": "ts-node scripts/normalize-blog-metadata.ts",
    "blog:auto-publish": "ts-node scripts/auto-blog-scheduler.ts",
    "blog:generate-today": "ts-node scripts/auto-blog-scheduler.ts --generate-today",
    "hypertune:generate": "npx hypertune",
    "hypertune:verify": "./scripts/verify-hypertune-setup.sh",
    "hypertune:test-api": "./scripts/test-hypertune-api.sh",
    "hypertune:check-schema": "./scripts/check-hypertune-schema.sh",
    "ai-gateway:setup": "./scripts/setup-ai-gateway.sh",
    "ai-gateway:pull": "vercel env pull",
    "bloom:run": "python3 scripts/bloom/run_bloom.py",
    "remotion:preview": "remotion preview remotion/Root.tsx",
    "remotion:deploy": "remotion lambda sites create remotion/Root.tsx --site-name=drsayuj-remotion-site",
    "remotion:render:local": "remotion render remotion/Root.tsx ConsultationPrep out/video.mp4",
    "remotion:render:services": "remotion render remotion/Root.tsx ServiceShowcase out/service-showcase.mp4",
    "remotion:render:dashboard": "remotion render remotion/Root.tsx OutcomeDashboard out/outcome-dashboard.mp4",
    "remotion:render:reel": "remotion render remotion/Root.tsx BlogToReel out/blog-reel.mp4",
    "remotion:render:intro": "remotion render remotion/Root.tsx DoctorIntro out/doctor-intro.mp4",
    "remotion:render:testimonials": "remotion render remotion/Root.tsx PatientTestimonial out/patient-testimonials.mp4",
    "remotion:render:all": "pnpm remotion:render:local && pnpm remotion:render:services && pnpm remotion:render:dashboard && pnpm remotion:render:reel && pnpm remotion:render:intro && pnpm remotion:render:testimonials"
  },
  "dependencies": {
    "@ai-sdk/openai": "3.0.18",
    "@ai-sdk/react": "3.0.51",
    "@flags-sdk/hypertune": "^0.3.2",
    "@google-cloud/bigquery": "^8.1.1",
    "@google/genai": "^0.6.0",
    "@hookform/resolvers": "^5.2.2",
